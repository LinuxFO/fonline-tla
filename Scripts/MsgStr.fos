// FOS Common Sort -1

hstring MakePropHashForMsg(CritterProperty prop)
{
    string str = hstring(prop);
    if (str.endsWith("Base"))
        return hstring(str.substr(0, str.length() - 4));
    return hstring(prop);
}

hstring Get(string strName)
{
    return hstring(strName);
}

